<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19survey">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covid19Survey">
        <activity android:name=".ReviewSurveyInputsActivity"
            android:parentActivityName=".SideEffectsActivity"/>
        <activity android:name=".SideEffectsActivity"
            android:parentActivityName=".VaccineTypeActivity"/>
        <activity android:name=".VaccineTypeActivity"
            android:parentActivityName=".GenderActivity"/>
        <activity android:name=".GenderActivity"
            android:parentActivityName=".CityActivity"/>
        <activity
            android:name=".BirthDateActivity"
            android:parentActivityName=".CityActivity" />
        <activity
            android:name=".CityActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>